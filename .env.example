# Digitransit API Configuration
# Get your API key from: https://portal-api.digitransit.fi/
VITE_DIGITRANSIT_API_KEY=your_api_key_here

# App Update Settings
VITE_UPDATE_INTERVAL=15000

# Default Map Location (Tartu, Estonia)
VITE_DEFAULT_LAT=58.3776
VITE_DEFAULT_LON=26.7290
VITE_DEFAULT_ZOOM=13

# EmailJS Configuration (Get these from https://dashboard.emailjs.com)
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key
